{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ],
  "functions": {
    "limits": {
      "maxDuration": 10,
      "maxMemory": 1024
    },
    "maxLambdaSize": "50mb"
  }
}